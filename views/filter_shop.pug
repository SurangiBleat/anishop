include header.pug

div(class='wrap')
	div(class='content')
		header(class='site-header')
			include site_header
			
		div(class='container')
			div(class='row')
				div(class='col col-md-12')
					h2 #{shop[0]['name']}
					div(class="paginate 1")
						div(class="items row")
							each val, index in goods
								include card.pug
						include pagination.html

	include nav
	include footer